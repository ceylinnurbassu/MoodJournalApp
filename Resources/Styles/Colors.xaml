<?xml version="1.0" encoding="UTF-8"?>
<?xaml-comp compile="true"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <Color x:Key="Primary">#FFCEE4B4</Color>
    <Color x:Key="PrimaryDark">#FF104B07</Color>
    <Color x:Key="PrimaryLight">#FFF6EFFE</Color>
    <Color x:Key="BackgroundLight">#FFFAF9FD</Color>
    <Color x:Key="NoteEnterenceColour">#C1D8FDBF</Color>
    <Color x:Key="Gray300">#2B6D7A0F</Color>
    <Color x:Key="Gray500">#6B7280</Color>
    <Color x:Key="Gray900">#111827</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}" />
</ResourceDictionary>
